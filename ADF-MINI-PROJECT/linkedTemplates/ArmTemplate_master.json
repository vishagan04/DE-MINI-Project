{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ADF-MINI-PROJECT"
		},
		"LS_sql_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'LS_sql'"
		},
		"MongoDbAtlas1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'MongoDbAtlas1'"
		},
		"LS_blob_task_2_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_blob_task_2'"
		},
		"LS_sql_task_2_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'LS_sql_task_2'"
		},
		"LS_sql_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "ss-client-order.database.windows.net"
		},
		"LS_sql_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "DB-mini"
		},
		"LS_sql_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "vishagan"
		},
		"MongoDbAtlas1_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "test"
		},
		"LS_sql_task_2_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "ss-client-order.database.windows.net"
		},
		"LS_sql_task_2_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "DB-task-2"
		},
		"LS_sql_task_2_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "vishagan"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ADF-MINI-PROJECT_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_sql_password": {
						"value": "[parameters('LS_sql_password')]"
					},
					"MongoDbAtlas1_connectionString": {
						"value": "[parameters('MongoDbAtlas1_connectionString')]"
					},
					"LS_blob_task_2_connectionString": {
						"value": "[parameters('LS_blob_task_2_connectionString')]"
					},
					"LS_sql_task_2_password": {
						"value": "[parameters('LS_sql_task_2_password')]"
					},
					"LS_sql_properties_typeProperties_server": {
						"value": "[parameters('LS_sql_properties_typeProperties_server')]"
					},
					"LS_sql_properties_typeProperties_database": {
						"value": "[parameters('LS_sql_properties_typeProperties_database')]"
					},
					"LS_sql_properties_typeProperties_userName": {
						"value": "[parameters('LS_sql_properties_typeProperties_userName')]"
					},
					"MongoDbAtlas1_properties_typeProperties_database": {
						"value": "[parameters('MongoDbAtlas1_properties_typeProperties_database')]"
					},
					"LS_sql_task_2_properties_typeProperties_server": {
						"value": "[parameters('LS_sql_task_2_properties_typeProperties_server')]"
					},
					"LS_sql_task_2_properties_typeProperties_database": {
						"value": "[parameters('LS_sql_task_2_properties_typeProperties_database')]"
					},
					"LS_sql_task_2_properties_typeProperties_userName": {
						"value": "[parameters('LS_sql_task_2_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "ADF-MINI-PROJECT_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'ADF-MINI-PROJECT_ArmTemplate_0')]"
			]
		},
		{
			"name": "ADF-MINI-PROJECT_ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_2.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'ADF-MINI-PROJECT_ArmTemplate_1')]"
			]
		}
	]
}