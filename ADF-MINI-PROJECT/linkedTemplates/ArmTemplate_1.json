				},
				"type": "MappingDataFlow",
				"typeProperties": {
					"sources": [
						{
							"dataset": {
								"referenceName": "DB_Prep_Joined_Transaction_User",
								"type": "DatasetReference"
							},
							"name": "source1"
						}
